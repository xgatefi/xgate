{
  "name": "@xgate/sdk",
  "version": "0.1.0",
  "description": "Client SDK for xGate Protocol with automatic x402 payment handling",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "xgate",
    "x402",
    "http-402",
    "micropayments",
    "usdc",
    "base",
    "coinbase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@xgate/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}

